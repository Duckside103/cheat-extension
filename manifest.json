{
  "manifest_version": 3,
  "name": "Ad-blocker",
  "version": "1.0.0",
  "description": "Ad-blocker",
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["./scripts/content.js"],
      "matches": ["https://www.onlinevarsity.com/*", "https://www.npmjs.com/*"]
    }
  ]
}
